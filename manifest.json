{
  "manifest_version": 2,
  "name": "Suprome",
  "version": "1.0.0",
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://www.supremenewyork.com/*"],
    "js": ["jquery.min.js", "content_script.js"],
    "run_at": "document_end"
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": [
    "declarativeContent",
    "webRequest",
    "*://www.supremenewyork.com/",
    "storage",
    "tabs"
  ]
}
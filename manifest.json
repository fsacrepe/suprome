{
  "manifest_version": 2,
  "name": "Suprome",
  "version": "1.0.0",
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://www.supremenewyork.com/*"],
    "js": ["jquery.min.js", "content_script.js"],
    "run_at": "document_end"
  }],
  "background": {
    "scripts": ["jquery.min.js", "background.js", "restock.js", "proxy.js", "cookies.js"]
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com https://cdn.jsdelivr.net https://stackpath.bootstrapcdn.com; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "declarativeContent",
    "webRequestBlocking",
    "webNavigation",
    "notifications",
    "webRequest",
    "cookies",
    "storage",
    "proxy",
    "tabs"
  ]
}